.siderBar .body .info .edt{ display:none; background:url(../images/s_ico2.png) no-repeat; width:14px; height:14px; text-indent:-9999em; position:absolute; top:68px; right:10px;}
.siderBar .body .info .siderIcoBtn{display:block; background:url(../images/s_ico0.png) no-repeat; width:14px; height:14px; text-indent:-9999em; position:absolute; top:10px; right:10px;}
.siderBar .body .info .avatar{ position:relative; width:95px; height:95px; margin-bottom:15px; }
.siderBar .body .info .avatar span{ display:block;/* background:url(../images/avatar_bg.png) no-repeat;*/ width:95px; height:95px; position:absolute; left:0; top:0;}
