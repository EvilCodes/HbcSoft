.login .ck a{  color:#938e8e;}
.login .submit{ display:block; height:50px; line-height:46px; background:url(../images/btn_blue.png); font-size:18px; color:#fff; border-radius:4px; border:none; width:299px; cursor:pointer;font-family: "微软雅黑", arial, Helvetica, sans-serif; text-indent:40px;}
.login .submit:hover{ opacity:0.8;}

