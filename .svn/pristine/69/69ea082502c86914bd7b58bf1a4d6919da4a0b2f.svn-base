.loginMain .footer p span{ padding:0 15px; }
.login{ background:url(../images/login_bg.png) no-repeat; width:394px; height:387px; position:absolute; right:50%; margin-right:-440px; top:205px; }
.login .head { height:65px; line-height:65px; text-align:center; font-size:24px;color:#fff;}
.login .body{ padding:30px 55px 30px 40px; padding-bottom:}
.login ul li{ margin:0; margin-bottom:11px; height:40px;}
.login ul li .text{ border:1px solid #E5E5E5; height:40px;  width:100%; border-radius:3px; text-indent:36px; font-size:14px; color:#666;font-family: "微软雅黑", arial, Helvetica, sans-serif;}
.login ul li .ico1{ background:#fff url(../images/ico1.png) no-repeat 9px center;}
.login ul li .ico2{ background:#fff url(../images/ico2.png) no-repeat 9px center;}
.login ul li .ico3{ background:#fff url(../images/ico3.png) no-repeat 9px center;}
.login ul li .button{height:41px; line-height:40px; background:#81C2FF; font-size:14px; color:#fff; border-radius:4px; border:none; width:130px; cursor:pointer; margin-left:15px;}
