header{
	border-bottom: 5px solid #701584;
}
/*头部导航划过样式*/
#pc .nav > li > a:focus, #pc .nav > li > a:hover{
	color: #701584;
}
/*导航元素样式*/
section #aside{
	background: #701584;
	border: 4px solid #f6f6f6;
	border-bottom: 0;
}
/*一级导航元素样式*/
#aside ul.subnav li a{
	color: #f6f6f6;
	border-bottom: 1px solid #5d0970;
	border-top: 1px solid #85209b;
}
/*二级导航元素样式*/
#aside ul.subnav li ul.menu{
	background: #85209b;
}
#aside ul.menu li a{
	border-bottom: 1px solid #701584;
	border-top: 1px solid #9d2eb5;
}
/*三级导航元素样式*/
#aside ul.subnav li ul.menu li .submenu{
	background: #9d2eb5;
}
#aside ul.submenu li a{
	border-bottom: 1px solid #85209b;
	border-top: 1px solid #ae3fc6;
}
/*导航元素折叠背景*/
.inactive{
	background: url(../../image/off1.png) no-repeat 175px center;
}
/*被选中导航元素背景*/
.navActive{
	background: #500361 url(../../image/on1.png) no-repeat 175px center;
}
.menuActive{
	background: #701584 url(../../image/on1.png) no-repeat 175px center;
}
.submenuActives{
	background: #85209b url(../../image/on1.png) no-repeat 175px center;
}
/*面板样式*/
.panel-primary{
	border-color: #701584;
}
.panel-primary > .panel-heading{
	background-color: #701584;
	border-color: #701584;
}
/*删行按钮*/
.delline{
	border: 1px solid #701584;
	background: #701584;
}
.delline:hover{
	border: 1px solid #701584;
	color: #701584;
}
/**/
.dl-item ol li a:hover{
	color: #701584;
}
.mainTitlet{
	background: #701584;
}
.btn-primary{
	background: #701584;
	border-color:#701584;
}
.pages>.pagination>.page>a{
	color: #701584;
}
.pages>.pagination>.active>a{
	background-color: #701584;
	border-color: #701584;
	color: #fff;
}
.pages>.pagination>li>a.first,.pages>.pagination>li>a.pre,.pages>.pagination>li>a.next,.pages>.pagination>li>a.last{
	color: #701584;
}
.pages>.pagination>li.disabled>a{
	color: #999;
}
.formcontrol:focus {
	border-color: #701584;
	outline: 0;
	-webkit-box-shadow: inset 0 1px 1px rgba(0,0,0,.075), 0 0 8px rgba(112, 21, 132, .6);
		box-shadow: inset 0 1px 1px rgba(0,0,0,.075), 0 0 8px rgba(112, 21, 132, .6);
}
.form-control:focus {
	border-color: #701584;
	outline: 0;
	-webkit-box-shadow: inset 0 1px 1px rgba(0,0,0,.075), 0 0 8px rgba(112, 21, 132, .6);
		box-shadow: inset 0 1px 1px rgba(0,0,0,.075), 0 0 8px rgba(112, 21, 132, .6);
}
.btn-primary{
	background: #701584;
	border-color: #500361;
}
.btn-primary:active,
.btn-primary:hover {
	background-color: #500361;
	border-color: #500361;
}
.btn-primary:active:hover{
	background-color: #500361;
	border-color: #500361;
}