.siderBar .body .info  p{ padding-left:27px; background:url(../images/s_ico1.png) no-repeat left center ; height:20px; line-height:20px; color:#dbdada;}
.siderIco .body .info .avatar{  height: 36px; margin-bottom:0px; margin-left: 4px; width: 36px;}
.siderIco .body .info .avatar span{width:36px; height:36px; position:absolute; left:0; top:0; background-size:100%;}
.siderIco .body .info .avatar img{ width:36px; height:36px;}
.siderIco .body .info  p{ padding-left:44px; font-size:0; background:url(../images/s_ico1.png) no-repeat center center ;}
.siderIco .body .info .edt{ display:none;}
.siderIco .body .info .siderIcoBtn{ display:block; background:url(../images/s_ico0.png) no-repeat; width:14px; height:14px; text-indent:-9999em; position:absolute; top:14px; right:18px;}


.siderBar .body .info  p span{ color:#33e4aa; padding:0 5px;}
.siderBar .foot{background:#464E78; padding:20px; border-top:1px solid #6B739F; color:#aaabab; line-height:22px; text-align:center;}
.siderIco  .foot{ display:none;}

