body {
	/*font-family: "微软雅黑", arial, Helvetica, sans-serif;*/
	font-family:"Microsoft Yahei", Arial, Helvetica, sans-serif;
	color: #666;
	font-size: 12px; background:#fff; height:100%;
	
}
a {
	color: #666;
	text-decoration: none;
}
a:hover { text-decoration:none;
