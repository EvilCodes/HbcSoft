header{
	border-bottom: 5px solid #bb0000;
}
/*头部导航划过样式*/
#pc .nav > li > a:focus, #pc .nav > li > a:hover{
	color: #bb0000;;
}
/*导航元素样式*/
section #aside{
	background: #bb0000;
	border: 4px solid #f6f6f6;
	border-bottom: 0;
}
/*一级导航元素样式*/
#aside ul.subnav li a{
	color: #f6f6f6;
	border-bottom: 1px solid #aa0000;
	border-top: 1px solid #cc0000;
}
/*二级导航元素样式*/
#aside ul.subnav li ul.menu{
	background: #cc0000;
}
#aside ul.menu li a{
	border-bottom: 1px solid #bb0000;
	border-top: 1px solid #dd0000;
}
/*三级导航元素样式*/
#aside ul.subnav li ul.menu li .submenu{
	background: #dd0000;
}
#aside ul.submenu li a{
	border-bottom: 1px solid #cc0000;
	border-top: 1px solid #ee0000;
}
/*导航元素折叠背景*/
.inactive{
	background: url(../../image/off1.png) no-repeat 175px center;
}
/*被选中导航元素背景*/
.navActive{
	background: #aa0000 url(../../image/on1.png) no-repeat 175px center;
}
.menuActive{
	background: #bb0000 url(../../image/on1.png) no-repeat 175px center;
}
.submenuActives{
	background: #cc0000 url(../../image/on1.png) no-repeat 175px center;
}
/*面板样式*/
.panel-primary{
	border-color: #bb0000;
}
.panel-primary > .panel-heading{
	background-color: #bb0000;
	border-color: #bb0000;
}
/*删行按钮*/
.delline{
	border: 1px solid #bb0000;
	background: #bb0000;
}
.delline:hover{
	border: 1px solid #bb0000;
	color: #bb0000;
}

/**/
.dl-item ol li a:hover{
	color: #bb0000;
}
.mainTitlet{
	background: #bb0000;
}
.btn-primary{
	background: #bb0000;
	border-color:#bb0000;
}

.pages>.pagination>.page>a{
	color: #bb0000;
}
.pages>.pagination>.active>a{
	background-color: #bb0000;
	border-color: #bb0000;
	color: #fff;
}
.pages>.pagination>li>a.first,.pages>.pagination>li>a.pre,.pages>.pagination>li>a.next,.pages>.pagination>li>a.last{
	color: #bb0000;
}
.pages>.pagination>li.disabled>a{
	color: #999;
}
.formcontrol:focus {
	border-color: #bb0000;
	outline: 0;
	-webkit-box-shadow: inset 0 1px 1px rgba(0,0,0,.075), 0 0 8px rgba(187, 0, 0, .6);
		box-shadow: inset 0 1px 1px rgba(0,0,0,.075), 0 0 8px rgba(187, 0, 0, .6);
}
.form-control:focus {
	border-color: #bb0000;
	outline: 0;
	-webkit-box-shadow: inset 0 1px 1px rgba(0,0,0,.075), 0 0 8px rgba(187, 0, 0, .6);
		box-shadow: inset 0 1px 1px rgba(0,0,0,.075), 0 0 8px rgba(187, 0, 0, .6);
}
.btn-primary{
	background: #bb0000;
	border-color: #aa0000;
}
.btn-primary:active,
.btn-primary:hover {
	background-color: #aa0000;
	border-color: #800000;
}
.btn-primary:active:hover{
	background-color: #900000;
	border-color: #700000;
}
