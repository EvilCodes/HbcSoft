/*登录*/
.loginMain{ background:#464E78 url(../images/bg.jpg) no-repeat center top;}
.loginMain .logo{ text-align:center; padding-top:78px;}
.loginMain .logo .p1{ text-align:center; font-size:38px; color:#fff;}
.loginMain .footer{ text-align:center; font-size:12px; color:#34c0e2;padding-top:500px; padding-bottom:50px;} 
.loginMain .footer p span{ padding:0 15px; }
.login{ background:url(../images/login_bg.png) no-repeat; width:394px; height:387px; position:absolute; right:50%; margin-right:-440px; top:185px; }
.login .head { height:65px; line-height:65px; text-align:center; font-size:24px;color:#fff;}
.login .body{ padding:30px 55px 30px 40px; padding-bottom:}
.login ul li{ margin:0; margin-bottom:11px; height:40px;}
.login ul li .text{ border:1px solid #E5E5E5; height:40px;  width:100%; border-radius:3px; text-indent:36px; font-size:14px; color:#666;font-family: "微软雅黑", arial, Helvetica, sans-serif;}
.login ul li .ico1{ background:#fff url(../images/ico1.png) no-repeat 9px center;}
.login ul li .ico2{ background:#fff url(../images/ico2.png) no-repeat 9px center;}
.login ul li .ico3{ background:#fff url(../images/ico3.png) no-repeat 9px center;}
.login ul li .button{height:41px; line-height:40px; background:#81C2FF; font-size:14px; color:#fff; border-radius:4px; border:none; width:130px; cursor:pointer; margin-left:15px;}
.login .ck{ line-height:20px; height:20px; padding:8px 0 12px 0; color:#938e8e;}
.login .ck input{ float:left; margin-right:5px; margin-top:3px;}
.login .ck span{ padding:0 12px;}
.login .ck a{  color:#938e8e;}
.login .submit{ display:block; height:50px; line-height:46px; background:url(../images/btn_blue.png); font-size:18px; color:#fff; border-radius:4px; border:none; width:299px; cursor:pointer;font-family: "微软雅黑", arial, Helvetica, sans-serif; text-indent:40px;}
.login .submit:hover{ opacity:0.8;}

/*主页面*/
.page{ background:#F5F5F5; height:100%; max-height:100%; overflow-y:hidden; _overflow-y:hidden; *overflow-y:hidden;}
#top{position:absolute; bottom:0; left:0; top:0; right: 0;  height:35px; width: 100%; z-index: 99999;}
#top .topRight{height:46px; line-height:46px; text-align:center; color:#fff; font-size:16px; border-right:1px solid #3585B7; background:#3585B7; font-weight:bold;position:absolute; bottom:0; left:0; top:0; right: 0; width:194px;}
#top .topRight a{color:#fff;}
#top .topLeft{position:absolute; bottom:0; left:194px; top:0; right: 0;  height:46px;}
.contentWrap{ background:#0b6fab;position: absolute; bottom:0; left:146px; top:46px; right: 0; }
.content{left:44px;}
.siderBar{ position:absolute; left:0; top:46px; bottom:0; width:146px;background:#003B6E; /*background:#464E78;555d88*/ height:100%;color:#dbdada; z-index:9999;}
.siderIco{width:44px;}

.nav-tip{cursor: pointer;width: 12px;background: url(../images/nav-tip.png) no-repeat;left:5px;top: 50%;height: 73px;position: absolute;background-position-x: 0; z-index:999;}

.Conframe{border: 1px solid #0b6fab;background: #0b6fab;position: absolute;left:6px;top:0px;overflow: auto;right: 0px;bottom:0px; z-index:999;padding: 0px;overflow-y:hidden; _overflow-y:hidden; *overflow-y:hidden;}
#Conframe{border: none;height: 100%;width:99%; margin-left:1%; }




.siderBar .head{height:40px; line-height:40px; text-align:center; color:#6b6d76; font-size:16px; border-right:1px solid #EEEFF2; background:#fff; font-weight:bold;}
.siderBar .body { }
.siderBar .body .info{ height:150px; position:relative; border-bottom:0px solid #3e83ba; padding-top:20px; padding-left:16px;}/*6B739F*/
