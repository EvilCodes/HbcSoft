/*主导航*/
.nav{height:69px; line-height:69px; text-align:center; color:#6f839e; font-size:14px; border-bottom:1px solid #3585B7; background:#fff;}
.nav ul{ text-align:right; overflow:hidden; background:#3585B7;}
.nav ul li{ display:inline-block; *display:inline; zoom:1; height:69px; line-height:69px;  }
.nav ul li.edtDel{ background:none;}
.nav ul li.edtDel em{position:absolute;display:block;width:0;height:0;
top:-8px;right:6px;border-bottom:8px solid #fff;border-right:8px solid transparent;border-left:8px solid transparent; display:none;}
.nav ul li.edtDel .edtDelT{   display: inline-block; line-height: 69px; position: absolute; right:6px;  top: 44px;  width: 116px; border:0px solid #ccc; background:#fff; border-radius:3px; display:none; box-shadow:0 0 5px;}
.nav ul li.edtDel:hover em{ display:block;}
.nav ul li.edtDel:hover .edtDelT{ display:block;}
.nav ul li.edtDel .edtDelT a{ color:#6f839e;display:block; text-align:center;}
.nav ul li.edtDel .edtDelT a:hover{ color:#6f839e;display:block; text-align:center; background:#a1daf4;}
.nav ul li.edtDel .edtDelT a:first-child:hover{ border-radius:5px 5px 0 0;}
.nav ul li a{ display:inline-block; *display:inline; zoom:1;height:69px; line-height:69px;padding:0 ; color:#fff;}
.nav ul li a:hover{ color:#f8f8f8; opacity:1;}
.nav ul li.noBorder{ background:none;}
.nav ul li i{  display:inline-block; *display:inline; zoom:1; width:21px; height:21px; margin-right:8px; background-position:center center; background-repeat:no-repeat; position:relative; top:5px;}
.nav ul li i.ico1{background-image:url(../images/n1.png);}
.nav ul li i.ico2{background-image:url(../images/n2.png);}
.nav ul li i.ico3{background-image:url(../images/n3.png);}
.nav ul li i.ico4{background-image:url(../images/n4.png);}
.nav ul li i.ico5{background-image:url(../images/n5.png);}
.nav ul li i.ico6{background-image:url(../images/n6.png);}
.nav ul li i.ico7{background-image:url(../images/n7.png);}
