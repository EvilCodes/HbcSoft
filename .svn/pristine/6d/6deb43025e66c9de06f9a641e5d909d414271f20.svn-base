@charset "utf-8";
*{
	margin: 0;
	padding: 0;
}
a{
	text-decoration: none;
}
li{
	list-style: none;
}
img{
	border: 0;
}
input{
	border: none;
}
header, section, footer, aside, nav, main, article, figure {
    display: block; 
}
h1,h2,h3,h4,h5,h6{
	font-weight: normal;
}
hr{
	border-color: #bbb;
}
.row{
	margin: 0;
}
.form-signin{
	max-width: 400px;
	padding: 25px;
	margin: 0 auto;
}
input {
	margin-bottom: 10px;
}
button{
	margin-top: 20px;
}
button#register{
	margin-top: 0;
	padding: 10px 50px;
}
h2{
	text-align: center;
	color: #337ab7;
	line-height: 2.5em;
	font-size: 30px;
}
h3{
	/*text-align: center;*/
	/*color: #337ab7;*/
	color: #333;
	line-height: 1.5em;
	font-size: 30px;
}
h3 span{
	font-size: 12px;
}
label{
	font-size: 14px;
	color: #555;
	padding-left: 15px;
	padding-top: 7px;
	/*font-weight: normal;*/
}
.input-group-addon{
	background: #fff;
}
.row a{
	color: #333;
	line-height: 36px;
	font-size: 14px;
}
.fujia,
.footer{
	text-align: center;
}
.fujia p,
.footer p{
	color: #333;
	line-height: 36px;
	margin-top: 20px;
	font-size: 16px;
}
.footer p{
	color: #555;
	font-size: 14px;
}
.fujia p a{
	padding-left: 20px;
}
.fujia a{
	color: #337ab7;
	line-height: 36px;
	margin-top: 20px;
	font-size: 16px;
}
.col-md-6,
.col-sm-5,
.col-xs-5{
	padding: 0;
}
.must{
	color: #ff0000;
}
.col-md-2,
.col-sm-2,
.col-xs-3{
	padding: 0;
	text-align: right;
}
.txt{
	padding: 8px 0 0 10px;
	font-size: 12px;
}
p.qiangdu{
	float: left;
	color: #ff0000;
}
ul{
	float: left;
}
ul li{
	float: left;
	width: 30px;
	height: 16px;
	background: #eee;
	margin: 2px 6px;
	line-height: 16px;
	border: 1px solid #aaa;
}
#slider {
  margin: 20px auto;
  width: 300px;
  height: 40px;
  position: relative;
  border-radius: 2px;
  border: 1px solid #dae2d0;
  overflow: hidden;
  text-align: center;
  user-select: none;
  -moz-user-select: none;
  -webkit-user-select: none;
}

#slider_bg {
  position: absolute;
  left: 0;
  top: 0;
  height: 100%;
  background-color: #337ab7;;
  z-index: 1;
}

#label {
  width: 46px;
  position: absolute;
  left: 0;
  top: 0;
  height: 38px;
  line-height: 38px;
  border: 1px solid #cccccc;
  background: #fff;
  z-index: 3;
  cursor: move;
  color: #337ab7;;
  font-size: 16px;
  font-weight: 900;
}

#labelTip {
  position: absolute;
  left: 0;
  width: 100%;
  height: 100%;
  font-size: 13px;
  font-family: 'Microsoft Yahei', serif;
  color: #787878;
  line-height: 38px;
  text-align: center;
  z-index: 2;
}
@media screen  and (max-width:1920px){
	.container{
		width: 1440px;
	}
}
@media screen  and (max-width:1440px){
	.container{
		width: 1170px;
	}
}